<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.talkhub.thprototipo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DadosCadastraisActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".CartaoActivity" />
        <activity android:name=".EquipeActivity" />
        <activity android:name=".HomeEquipeActivity" />
        <activity android:name=".ProjetoActivity" />
        <activity android:name=".SettingsEquipeActivity"></activity>
    </application>

</manifest>